[
	"node-app-1": {
		"image": "github.com/BenHall/docker-node-example",
		"build_required": true,
		"ports": [{"guest": 3000, "host": 46169}]
	},
	"nginx_root": {
		"image": "github.com/dockerfile/nginx",
		"ports": [{"guest": 80, "host": 80}],
		"links": ["node-app-1"],
		"map_directories": [
			{"guest": "/data", host:"/Users/ben/Desktop/nginx/www"}
			{"guest": "/etc/nginx/sites-enabled", host:"/Users/ben/Desktop/nginx/sites-enabled"}
			{"guest": "/var/log/nginx", host:"/Users/ben/Desktop/nginx/logs"}
		],
		"config": [
			{
				"target": "node-app-1",
				"domain": "localhost",
				"proxy", true,
				"ports": [{"guest": 46169, "expose": 80}]
			}
		]
	}
]